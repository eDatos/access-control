<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>Data sources configuration</comment>

    <!-- ORACLE -->
    <entry key="metamac.access.control.db.url">jdbc:oracle:thin:@FILL_HOST:FILL_PORT:XE</entry>
    <entry key="metamac.access.control.db.driverName">oracle.jdbc.OracleDriver</entry>
    <entry key="metamac.access.control.db.username">FILL_USERNAME</entry>
    <entry key="metamac.access.control.db.password">FILL_PASSWORD</entry>
    <entry key="metamac.access.control.db.dialect">org.siemac.metamac.hibernate.dialect.Oracle10gDialectMetamac</entry>

</properties>
